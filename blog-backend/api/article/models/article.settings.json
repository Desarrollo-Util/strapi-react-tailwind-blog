{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "publishedAt": {
      "type": "date",
      "required": true
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ],
      "required": true,
      "default": "draft",
      "private": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "category": {
      "model": "category",
      "via": "articles"
    },
    "cardImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "author": {
      "model": "writer",
      "via": "articles"
    },
    "articleContent": {
      "type": "dynamiczone",
      "components": [
        "article-content.parrafo",
        "article-content.titulo"
      ]
    }
  }
}
